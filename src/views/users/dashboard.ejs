<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CryptoApp Dashboard</title>
</head>
<body>
    <form method="POST" action="/users/symbols/add">
        <input type="text" name="symbol" placeholder="crypto token">
        <button>Add</button>
    </form>

    <br/>
    <br/>

    <table>
        <thead>
            <tr>
                <th>Symbol</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            <% symbolValues.forEach(userSymbol => { %>
                <tr>
                    <td><%= userSymbol.symbol %></td>
                    <td><%= userSymbol.value %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>